<p style="color: green"><%= notice %></p>

<% content_for :title, "Creators" %>

<h1>Creators</h1>

<div id="creators">
  <% @creators.each do |creator| %>
    <%= render creator %>
    <p>
      <%= link_to "Show this creator", creator %>
    </p>
  <% end %>
</div>

<%= link_to "New creator", new_creator_path %>
